<?xml version="1.0"?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example1"
    versionCode = "10"
    version     = "1.0.1" >

  <configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
  </system.web>
  </configuration>
  
  <!-- versionCode is optional and Android only -->

  <name>P_Golf Club Booking 2.2</name>

  <description>
    An example for phonegap build docs.
  </description>

    <!-- Android -->
    <preference name="orientation" value="portrait" />
    <icon src="images/icon/android/ldpi/ic_launcher.png" gap:platform="android" gap:density="ldpi" />
    <icon src="images/icon/android/mdpi/ic_launcher.png" gap:platform="android" gap:density="mdpi" />
    <icon src="images/icon/android/hdpi/ic_launcher.png" gap:platform="android" gap:density="hdpi" />
    <icon src="images/icon/android/xhdpi/ic_launcher.png" gap:platform="android" gap:density="xhdpi" />

  <!-- IOS -->
  <icon src="images/icon/ios/icon.png" gap:platform="ios" width="57" height="57" />
  <icon src="images/icon/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
  <icon src="images/icon/ios/Icon@2x.png" gap:platform="ios" width="114" height="114" />

  <!-- retina iPad support: PhoneGap 2.5.0+ only -->
  <icon src="images/icon/ios/icon-72@2x.png" gap:platform="ios" width="144" height="144" />
  <gap:plugin name="com.phonegap.plugins.pushplugin" version="2.1.1" />
  
</widget>